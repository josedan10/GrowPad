{
  "name": "growpad",
  "version": "1.0.0",
  "description": "A fun app to organize your activities",
  "main": "index.jsx",
  "scripts": {
    "start": "concurrently \"npm run compile-css\" \"react-scripts start\"",
    "start-react": "react-scripts start",
    "test-scraper": "nodemon src/scraper/index.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch-css": "nodemon -e sass -x \"npm run compile-css\"",
    "compile-css": "node-sass --watch --output ./src/css/ ./src/sass/index.scss",
    "lint": "eslint ./src/**/*.js ./src/**/*.jsx --fix --max-warnings=0"
  },
  "author": "Jose Quintero",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "firebase": "^5.7.2",
    "firebase-admin": "^7.0.0",
    "firebase-functions": "^2.2.0",
    "googleapis": "^27.0.0",
    "objects-to-csv": "^1.2.1",
    "gh-pages": "^2.0.1",
    "history": "^4.7.2",
    "js-beautify": "^1.9.1",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-redux-firebase": "^3.0.0-alpha.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-firestore": "^0.6.2",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "sass-lint": "^1.12.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1"
  },
  "homepage": "https://josedan10.github.io/GrowPad/"
}
