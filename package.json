{
  "name": "organize-app-website",
  "version": "1.0.0",
  "description": "A fun app to organize your activities",
  "main": "index.jsx",
  "scripts": {
    "start": "concurrently \"npm run compile-css\" \"react-scripts start\"",
    "start-react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch-css": "nodemon -e sass -x \"npm run compile-css\"",
    "compile-css": "node-sass --watch --output ./src/css/ ./src/sass/index.scss",
    "lint": "eslint \"**/*.js\" --fix --fix-type layout --max-warnings=0"
  },
  "author": "Jose Quintero",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "firebase": "^5.7.2",
    "firebase-admin": "^7.0.0",
    "firebase-functions": "^2.2.0",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-redux-firebase": "^3.0.0-alpha.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.2",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-firestore": "^0.6.2",
    "redux-thunk": "^2.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1"
  }
}
